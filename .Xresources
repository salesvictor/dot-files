! Extensions
URxvt.perl-ext-common: default,fullscreen,url-select,keyboard-select,matcher,resize-font

!! Fullscreen
URxvt.keysym.F11: perl:fullscreen:switch

!! url-select
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.launcher: google-chrome-stable
URxvt.url-select.underline: true

!! keyboard-select
URxvt.keysym.M-Escape: perl:keyboard-select:activate

!! matcher
URxvt.url-launcher: google-chrome-stable
URxvt.matcher.button: 1

!! resize-font
URxvt.resize-font.smaller: C-j
URxvt.resize-font.bigger: C-k
URxvt.resize-font.step: 1

! Rofi
! State:           'bg'           'fg'     'bgalt',       'hlbg',  'hlfg'
!rofi.color-window: argb:c82d303b, #7c8389, #1d1f21
